+++ b/drivers/dsp/bridge/pmgr/dbll.c
@@ -836,9 +836,8 @@ void dbll_unload(struct dbll_library_obj *lib, struct dbll_attrs *attrs)
 					    &zl_lib->symbol.dl_symbol,
