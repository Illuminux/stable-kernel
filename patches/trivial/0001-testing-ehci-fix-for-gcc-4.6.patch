+++ b/include/linux/usb/ehci_def.h
@@ -194,7 +194,7 @@ struct ehci_dbg_port {
 	u32	data47;
 	u32	address;
 #define DBGP_EPADDR(dev, ep)	(((dev)<<8)|(ep))
-} __attribute__ ((packed));
+} __attribute__ ((packed,aligned(__alignof__(int))));
 
