From cf057e6b8120c1638ffe164b4e1d13ea5b6125f2 Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Thu, 16 Sep 2010 14:55:10 -0500
Subject: [PATCH 13/22] ARM OMAP Touchbook upstream rename touchbook_irq

---
 arch/arm/mach-omap2/board-omap3touchbook.c |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/mach-omap2/board-omap3touchbook.c b/arch/arm/mach-omap2/board-omap3touchbook.c
index 11cbb8f..dee3965 100644
--- a/arch/arm/mach-omap2/board-omap3touchbook.c
+++ b/arch/arm/mach-omap2/board-omap3touchbook.c
@@ -472,7 +472,7 @@ static struct omap_board_mux board_mux[] __initdata = {
 #define board_mux	NULL
 #endif
 
-static void __init omap3_touchbook_init_irq(void)
+static void __init touchbook_init_irq(void)
 {
 	omap2_init_common_hw(mt46h32m32lf6_sdrc_params,
 			     mt46h32m32lf6_sdrc_params);
@@ -604,7 +604,7 @@ MACHINE_START(TOUCHBOOK, "OMAP3 touchbook Board")
 	.boot_params	= 0x80000100,
 	.map_io		= omap3_map_io,
 	.reserve	= omap_reserve,
-	.init_irq	= omap3_touchbook_init_irq,
+	.init_irq	= touchbook_init_irq,
 	.init_machine	= super_init,
 	.timer		= &omap_timer,
 MACHINE_END
-- 
