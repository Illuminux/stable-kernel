From c139ed2a4a9983305d69b60b43e347bee28a3270 Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Thu, 16 Sep 2010 15:00:00 -0500
Subject: [PATCH 15/22] ARM OMAP Touchbook upstream rename both i2c boardinfo

---
 arch/arm/mach-omap2/board-omap3touchbook.c |   10 ++++------
 1 files changed, 4 insertions(+), 6 deletions(-)

diff --git a/arch/arm/mach-omap2/board-omap3touchbook.c b/arch/arm/mach-omap2/board-omap3touchbook.c
index 77df745..b477b8d 100644
--- a/arch/arm/mach-omap2/board-omap3touchbook.c
+++ b/arch/arm/mach-omap2/board-omap3touchbook.c
@@ -334,7 +334,7 @@ static struct twl4030_platform_data touchbook_twldata = {
 	.vpll2		= &touchbook_vpll2,
 };
 
-static struct i2c_board_info __initdata touchbook_i2c_boardinfo[] = {
+static struct i2c_board_info __initdata touchbook_i2c_boardinfo_1[] = {
 	{
 		I2C_BOARD_INFO("twl4030", 0x48),
 		.flags = I2C_CLIENT_WAKE,
@@ -343,7 +343,7 @@ static struct i2c_board_info __initdata touchbook_i2c_boardinfo[] = {
 	},
 };
 
-static struct i2c_board_info __initdata touchBook_i2c_boardinfo[] = {
+static struct i2c_board_info __initdata touchbook_i2c_boardinfo_3[] = {
 	{
 		I2C_BOARD_INFO("bq27200", 0x55),
 	},
@@ -352,12 +352,10 @@ static struct i2c_board_info __initdata touchBook_i2c_boardinfo[] = {
 static int __init touchbook_i2c_init(void)
 {
 	/* Standard TouchBook bus */
-	omap_register_i2c_bus(1, 2600, touchbook_i2c_boardinfo,
-			ARRAY_SIZE(touchbook_i2c_boardinfo));
+	omap_register_i2c_bus(1, 2600, touchbook_i2c_boardinfo_1, ARRAY_SIZE(touchbook_i2c_boardinfo_1));
 
 	/* Additional TouchBook bus */
-	omap_register_i2c_bus(3, 100, touchBook_i2c_boardinfo,
-			ARRAY_SIZE(touchBook_i2c_boardinfo));
+	omap_register_i2c_bus(3, 100, touchbook_i2c_boardinfo_3, ARRAY_SIZE(touchbook_i2c_boardinfo_3));
 
 	return 0;
 }
-- 
