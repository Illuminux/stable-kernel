From 4485404d70a5486c94a40a1eaea8dfc2f6fc8b3e Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Thu, 16 Sep 2010 12:18:29 -0500
Subject: [PATCH 08/22] ARM OMAP Touchbook upstream omap3touchbook to touchbook

---
 arch/arm/mach-omap2/board-omap3touchbook.c |   16 ++++++++--------
 1 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/arch/arm/mach-omap2/board-omap3touchbook.c b/arch/arm/mach-omap2/board-omap3touchbook.c
index 2be6441..96da76d 100644
--- a/arch/arm/mach-omap2/board-omap3touchbook.c
+++ b/arch/arm/mach-omap2/board-omap3touchbook.c
@@ -63,7 +63,7 @@
 
 unsigned long touchbook_revision;
 
-static struct mtd_partition omap3touchbook_nand_partitions[] = {
+static struct mtd_partition touchbook_nand_partitions[] = {
 	/* All the partition sizes are listed in terms of NAND block size */
 	{
 		.name		= "X-Loader",
@@ -93,10 +93,10 @@ static struct mtd_partition omap3touchbook_nand_partitions[] = {
 	},
 };
 
-static struct omap_nand_platform_data omap3touchbook_nand_data = {
+static struct omap_nand_platform_data touchbook_nand_data = {
 	.options	= NAND_BUSWIDTH_16,
-	.parts		= omap3touchbook_nand_partitions,
-	.nr_parts	= ARRAY_SIZE(omap3touchbook_nand_partitions),
+	.parts		= touchbook_nand_partitions,
+	.nr_parts	= ARRAY_SIZE(touchbook_nand_partitions),
 	.dma_channel	= -1,		/* disable DMA in OMAP NAND driver */
 	.nand_setup	= NULL,
 	.dev_ready	= NULL,
@@ -486,7 +486,7 @@ static struct platform_device *omap3_touchbook_devices[] __initdata = {
 	&touchbook_dss_device,
 };
 
-static void __init omap3touchbook_flash_init(void)
+static void __init touchbook_flash_init(void)
 {
 	u8 cs = 0;
 	u8 nandcs = GPMC_CS_NUM + 1;
@@ -511,10 +511,10 @@ static void __init omap3touchbook_flash_init(void)
 	}
 
 	if (nandcs < GPMC_CS_NUM) {
-		omap3touchbook_nand_data.cs = nandcs;
+		touchbook_nand_data.cs = nandcs;
 
 		printk(KERN_INFO "Registering NAND on CS%d\n", nandcs);
-		if (gpmc_nand_init(&omap3touchbook_nand_data) < 0)
+		if (gpmc_nand_init(&touchbook_nand_data) < 0)
 			printk(KERN_ERR "Unable to register NAND device\n");
 	}
 }
@@ -580,7 +580,7 @@ static void __init omap3_touchbook_init(void)
 	omap3_ads7846_init();
 	usb_musb_init(&musb_board_data);
 	usb_ehci_init(&ehci_pdata);
-	omap3touchbook_flash_init();
+	touchbook_flash_init();
 
 	/* Ensure SDRC pins are mux'd for self-refresh */
 	omap_mux_init_signal("sdrc_cke0", OMAP_PIN_OUTPUT);
-- 
1.7.1

