From 26b913c0cdc3e36f1058e4c30ab9bc28114247f1 Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Thu, 16 Sep 2010 16:43:34 -0500
Subject: [PATCH 5/5] ARM OMAP Touchbook upstream echi

---
 arch/arm/mach-omap2/board-omap3touchbook.c |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/mach-omap2/board-omap3touchbook.c b/arch/arm/mach-omap2/board-omap3touchbook.c
index 001f3e6..2b0a8ac 100644
--- a/arch/arm/mach-omap2/board-omap3touchbook.c
+++ b/arch/arm/mach-omap2/board-omap3touchbook.c
@@ -495,7 +495,7 @@ static void __init touchbook_flash_init(void)
 	}
 }
 
-static const struct ehci_hcd_omap_platform_data ehci_pdata __initconst = {
+static const struct ehci_hcd_omap_platform_data touchbook_ehci_pdata __initconst = {
 
 	.port_mode[0] = EHCI_HCD_OMAP_MODE_PHY,
 	.port_mode[1] = EHCI_HCD_OMAP_MODE_PHY,
@@ -630,7 +630,7 @@ static void __init touchbook_init(void)
 	gpio_direction_output(176, true);
 
 	usb_musb_init(&musb_board_data);
-	usb_ehci_init(&ehci_pdata);
+	usb_ehci_init(&touchbook_ehci_pdata);
 	touchbook_flash_init();
 
 	/* Ensure SDRC pins are mux'd for self-refresh */
-- 
