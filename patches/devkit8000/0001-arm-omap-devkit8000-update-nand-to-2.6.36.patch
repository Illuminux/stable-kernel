+++ b/arch/arm/mach-omap2/board-devkit8000.c
@@ -603,7 +603,7 @@ static void __init devkit8000_flash_init(void)
 			(gpmc_base_add);
 
 		printk(KERN_INFO "Registering NAND on CS%d\n", nandcs);
