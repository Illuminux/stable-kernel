From 93ec403fa4aad569fa8b29a0ffd23dfb40ddc09d Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Thu, 6 Dec 2012 20:32:33 -0600
Subject: [PATCH 11/11] wip: ARM: OMAP: Beagle: add LSR COM6L Adapter Board
 support

Signed-off-by: Robert Nelson <robertcnelson@gmail.com>
---
 arch/arm/mach-omap2/board-omap3beagle.c |    5 +++--
 1 file changed, 3 insertions(+), 2 deletions(-)

diff --git a/arch/arm/mach-omap2/board-omap3beagle.c b/arch/arm/mach-omap2/board-omap3beagle.c
index 21ad9d6..c24ac56 100644
--- a/arch/arm/mach-omap2/board-omap3beagle.c
+++ b/arch/arm/mach-omap2/board-omap3beagle.c
@@ -1116,7 +1116,7 @@ static void __init omap3_beagle_init(void)
 	}
 
 #if defined(CONFIG_WL12XX) || defined(CONFIG_WL12XX_MODULE)
-	if (!strcmp(expansionboard_name, "bbtoys-wifi"))
+	if ((!strcmp(expansionboard_name, "bbtoys-wifi")) || (!strcmp(expansionboard_name, "lsr-com6l-adpt")))
 	{
 		expansion_config.mmc_settings = EXPANSION_MMC_WIFI;
 	}
@@ -1211,8 +1211,9 @@ static void __init omap3_beagle_init(void)
 	}
 
 #if defined(CONFIG_WL12XX) || defined(CONFIG_WL12XX_MODULE)
-	if (!strcmp(expansionboard_name, "bbtoys-wifi"))
+	if ((!strcmp(expansionboard_name, "bbtoys-wifi")) || (!strcmp(expansionboard_name, "lsr-com6l-adpt")))
 	{
+		printk(KERN_INFO "Beagle expansionboard: initializing wl12xx platform\n");
 		omap_beagle_wlan_data.irq = gpio_to_irq(OMAP_BEAGLE_WLAN_IRQ_GPIO);
 		if (wl12xx_set_platform_data(&omap_beagle_wlan_data))
 			pr_err("error setting wl12xx data\n");
-- 
